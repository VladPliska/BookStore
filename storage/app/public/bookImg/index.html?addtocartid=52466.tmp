      this.setCustomValidity("Номер набран неправильно");
    	}
    });
		$('input[name="getwrd"]').on('input invalid', function() {
			this.setCustomValidity('');
      if (this.validity.valueMissing) {
        this.setCustomValidity("Введите вашу фамилию");
      }
    	if (this.validity.typeMismatch) {
      	this.setCustomValidity("Не соответствует типу");
    	}
    	if (this.validity.patternMismatch) {
      this.setCustomValidity("Введите минимум 2 буквы");
    	}
    });
	if (window.location.hash && window.location.hash == '#_=_') 
	{
        window.location.href = window.location.href.split('#_=_')[0];
    }
});
    
