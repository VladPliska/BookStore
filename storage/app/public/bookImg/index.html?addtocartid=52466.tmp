	<script type="text/javascript">
		changeHeadInfo = function(param) {
			var show = 1;
			var ga_block = ga_banner = '';
			if ($('#head_info').is(':visible')) { 
				if (param != 2) {
					$('#head_info').hide('slow');
					$('#info_action_help .ai_bl_open').show('slow');
					show = 0;
					ga_block = 'Close Block';
				} else {
					ga_block = 'Go to top';
				}
			} else {
				$('#head_info').show('slow');
				$('#info_action_help .ai_bl_open').hide('slow');
				ga_block = 'Open Block';
			}
			switch(param) {
				case 1 :
					ga_banner = 'Header Banner';
					break;
				case 2 :
					ga_banner = 'Middle Banner';
					$('html,body').animate({scrollTop : 0}, 1000);
					break;
				case 3 :
					ga_banner = 'Info Block';
					break;
			}
			if(typeof(ga)!='undefined' && ga_block) {
				ga('send','event','Recrutment',ga_banner,'-0 ' + ga_block);
			}
			if (ga_block) {
				$.ajax({
					url: '/cart/bloks/recrutSite/_ajax_set_sidebar_show.php',
					type: "POST",
					data: {show : show}
				});
			}
		}
		$(function() {
			$('#dbody').on('click', '.change_activ_offer', function() {
				if ($(this).hasClass('activ0')) {
					if(typeof(ga)!='undefined') {
						ga('send','event','Recrutment','Info Block','-0 ' + 'Close block and diactivate');
					}
					changeActivOffer(0);
				} else if ($(this).children('.ref_join').length) {
					var param = $(this).children('.ref_join').text();
					sendToJoin(param);
				} else {
					if(typeof(ga)!='undefined') {
						if ($(this).hasClass('cao_top')) {
							ga('send','event','Recrutment','Info Block','-0 ' + 'Activate discount');
						}
						if ($(this).hasClass('cao_card')) {
							ga('send','event','Recrutment','Product page banner','-0 ' + 'Activate discount');
						}
					}
					changeActivOffer(1);
				}
			});
			$('.close_bl_noact').on('click', function() {
				changeHeadInfo(3);
				event.stopPropagation();
			});
		})
		sendToJoin = function(ref) {
			var htmlForm = '<form id="send_to_join" method="post" action="/catalog/join/' + ref + 
				'"><input type="hidden" name="join_add_url" value="order_done=50" />';
			htmlForm += '</form>';
			$('#dbody').append(htmlForm);
			$('#send_to_join').submit();
		}
		changeActivOffer = function(activ) {
			var htmlForm = '<form id="recrutment_site" method="post"><input type="hidden" name="recrutment_site_activ" value="' + activ + '" />';
			htmlForm += '</form>';
			$('#dbody').append(htmlForm);
			$.ajax({
				url: '/cart/bloks/recrutSite/_ajax_set_sidebar_show.php',
				type: "POST",
				data: {show : activ},
				success: function() {
					$('#recrutment_site').submit();
				}
			});
		}
	</script>
