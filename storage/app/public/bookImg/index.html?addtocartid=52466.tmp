<script>
	$().ready(function() {
	
		$(".logpopup #nncField").bind('keyup keydown blur focus', function() {
			$(this).val($(this).val().replace(/[^0-9]/g, ''));
			if ($(this).val().length > 0) {
				$(this).addClass('has-value');
			} else {
				$(this).removeClass('has-value');
			}
		});
	
		$("#nncField").keyup(function() {	
			if ($("#nncField").val().length > 0 && !/^\d+$/.test($("#nncField").val())) {
				if (!$("#errorToolTipNNC").is('div')) {
					var tooltip = '<div id="errorToolTipNNC" align="center"><p>Только цифры!</p></div>';
					var pageX = $('#nncField').offset().left;
					var pageY = $('#nncField').offset().top;
					
					$('body').append(tooltip);
					$("#errorToolTipNNC").hide();
					$("#errorToolTipNNC").css('top', pageY + 'px').css('left', (pageX - 200) + 'px').fadeIn(500);
				}
				$(this).val($(this).val().replace(/[^0-9]/g, ''));
				$("#errorToolTipNNC").fadeOut(500).delay(500).queue(function() {	
					$(this).remove();
				});				
			} else {
				$("#errorToolTipNNC").fadeOut(500).delay(500).queue(function() {
					$(this).remove();
				});				
			}			
		});

		$("#snameField").keyup(function() {
			if ($("#snameField").val().length > 0 && !/^[абгвдеёжзийклмнопрстуфхцчшщъыьэюя\-]+$/i.test($("#snameField").val())) {
				if (!$("#errorToolTipName").is('div')) {
					var tooltip = '<div id="errorToolTipName" align="center"><p>Только кириллица!</p></div>';
					var pageX = $('#snameField').offset().left;
					var pageY = $('#snameField').offset().top;
					
					$('body').append(tooltip);
					$("#errorToolTipName").hide();
					$("#errorToolTipName").css('top', pageY + 'px').css('left', (pageX - 200) + 'px').fadeIn(500);
				}
			} else {
				$("#errorToolTipName").fadeOut(500).delay(500).queue(function() {
					$(this).remove();
				});				
			}
		});
    $('input[name="getusr"]').on('input invalid', function() {
			this.setCustomValidity('');
      if (this.validity.valueMissing) {
        this.setCustomValidity("Введите ваш клубный номер");
      }
